	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0px"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "14px",
					"fontStyle": "normal",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"core/columns": {
				"spacing": {
					"padding": {
						"left": "20px",
						"right": "20px"
					}
				}
			},
			"core/heading": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontStyle": "normal",
					"fontWeight": "600",
					"textTransform": "uppercase"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var:preset|font-family|heebo"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var:preset|color|custom-secondary"
							}
						},
						"color": {
							"text": "var:preset|color|custom-accent"
						}
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "var:preset|font-size|x-large"
				}
			}
		},
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--custom-accent)",
						"text": "var(--wp--preset--color--custom-contrast)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--custom-contrast)",
						"text": "var(--wp--preset--color--custom-base)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--custom-secondary)",
						"text": "var(--wp--preset--color--custom-contrast)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--custom-contrast)"
					}
				},
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--custom-accent)",
					"text": "var:preset|color|custom-contrast"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var:preset|font-family|cormorant",
					"fontSize": "58px",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "36px"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "20px"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "20px"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "13px"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "Cormorant"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|custom-primary"
					}
				},
				"color": {
					"text": "var:preset|color|custom-accent"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "header",
			"name": "header-large"
		},
		{
			"area": "header",
			"name": "header-light"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}
